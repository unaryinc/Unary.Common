{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
	{
		"name": "Debug",
		"type": "cppdbg",
		"request": "launch",
		"args": [],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": true,
		"windows":
		{
			"program": "${workspaceFolder}\\Unary.Make.Debug.exe",
			"miDebuggerPath": "${workspaceFolder}\\..\\Unary.MinGW\\bin\\gdb.exe",
		},
		"linux":
		{
			"program": "${workspaceRoot}/Unary.Make.Debug.out",
			"miDebuggerPath": "gdb",
		},
		"MIMode": "gdb",
		"setupCommands": [
		{
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		}
		],
		"preLaunchTask": "Debug"
	},
	{
		"name": "Release",
		"type": "cppdbg",
		"request": "launch",
		"args": [],
		"stopAtEntry": false,
		"cwd": "${workspaceFolder}",
		"environment": [],
		"externalConsole": true,
		"windows":
		{
			"program": "${workspaceFolder}\\Unary.Make.Release.exe",
			"miDebuggerPath": "${workspaceFolder}\\..\\Unary.MinGW\\bin\\gdb.exe",
		},
		"linux":
		{
			"program": "${workspaceRoot}/Unary.Make.Release.out",
			"miDebuggerPath": "gdb",
		},
		"MIMode": "gdb",
		"setupCommands": [
		{
			"description": "Enable pretty-printing for gdb",
			"text": "-enable-pretty-printing",
			"ignoreFailures": true
		}
		],
		"preLaunchTask": "Release"
	},
	{
		"name": "Clean",
		"type": "cppdbg",
		"request": "launch",
		"cwd": "${workspaceFolder}",
		"externalConsole": true,
		"windows":
		{
			"program": "${workspaceFolder}\\..\\Unary.MinGW\\bin\\mingw32-make.exe",
			"args": 
			[
				"-f",
				"${workspaceFolder}\\Makefile",
				"clean"
			]
		},
		"linux":
		{
			"program": "make",
			"args": 
			[
				"-f",
				"${workspaceFolder}\\Makefile",
				"clean"
			]
		}
	}
	]
}